<h3>Hi there, <%= link_to @user.username, user_path %>! </h3>
<%= render 'layouts/flash_messages' %>
<%= link_to "Create a new list", new_user_list_path(@user) %>
<p>
  <ul>
    <% @user.lists.each do |list| %>
      <li><%= link_to list.title,  user_list_path(@user, list) %></li>
   
    <% end %>
  </ul>
</p> 