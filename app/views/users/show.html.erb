<h3>Hi there, <%= link_to @user.username, user_path %>! </h3>
<%= link_to "Create a new list", new_user_list_path(@user) %> |
<%= link_to 'Logout', logout_path %>
<p>
  <ul>
    <% @user.lists.each do |list| %>
      <li><%= link_to list.title,  user_list_path(@user, list) %> |
      <%= link_to "edit", edit_user_list_path(@user, list) %></li>
    <% end %>
  </ul>
</p> 